{
  "name": "devs-vscode",
  "displayName": "devs-vscode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:localResource",
    "onView:remoteResource"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "Serverless-Devs-explorer": [
        {
          "id": "localResource",
          "name": "local resource"
        },
        {
          "id": "remoteResource",
          "name": "remote resource"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "Serverless-Devs-explorer",
          "title": "Serverless-Devs",
          "icon": "media/dep.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "serverless-devs.config",
        "title": "Add Account"
      },
      {
        "command": "serverless-devs.init",
        "title": "Create Function",
        "icon": {
          "light": "media/light/plus.svg",
          "dark": "media/dark/plus.svg"
        }
      },
      {
        "command": "serverless-devs.public.demo",
        "title": "Create Demo",
        "icon": {
          "light": "media/light/plus.svg",
          "dark": "media/dark/plus.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "serverless-devs.config",
          "when": "view == localResource"
        },
        {
          "command": "serverless-devs.init",
          "group": "navigation",
          "when": "view == localResource"
        }
      ],
      "view/item/context": [
        {
          "command": "serverless-devs.init",
          "when": "view == localResource",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@serverless-devs/core": "^0.1.36",
    "fs-extra": "^10.0.1"
  }
}
